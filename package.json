{
  "name": "asktoni-client",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "RedTn <redmond.tran@gmail.com>",
  "private": true,
  "scripts": {
    "build": "node build/build.js",
    "ci-test": "npm run unit",
    "clean": "rm -rf node_modules dist && yarn install",
    "deploy": "rm -rf dist && npm run build && surge dist asktoni.surge.sh",
    "deploy-staging": "rm -rf dist && npm run build && surge dist asktoni-staging.surge.sh",
    "dev": "node build/dev-server.js",
    "e2e": "node test/e2e/runner.js",
    "lint": "eslint . --max-warnings 0 --ext .js,.vue src config test/unit/specs test/e2e/specs",
    "lint-fix": "npm run prettier && eslint . --fix --max-warnings 0 --ext .js,.vue src config test/unit/specs test/e2e/specs",
    "lint-fix-simple": "eslint . --fix --max-warnings 0 --ext .js,.vue src config test/unit/specs test/e2e/specs",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --tab-width 4 --print-width 100 --write \"{src,test/unit/specs,test/e2e/specs,config}/**/!(.eslintrc).js\"",
    "start": "node build/dev-server.js",
    "test": "npm run unit && npm run e2e",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run"
  },
  "lint-staged": {
    "{src,test/unit/specs,test/e2e/specs,config}/**/!(.eslintrc).js": [
      "npm run prettier",
      "eslint . --fix --max-warnings 0 --ext .js,.vue src config test/unit/specs test/e2e/specs",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.17.0",
    "leaflet": "^1.2.0",
    "leaflet.icon.glyph": "^0.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "vue": "^2.3.3",
    "vue-material": "^1.0.0-beta-8",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "chromedriver": "^2.32.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^6.0.3",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.1.3",
