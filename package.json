    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.2",
    "lint-staged": "^6.0.0",
    "lolex": "^2.1.3",
    "mocha": "^5.0.0",
    "moment": "^2.18.1",
    "nightwatch": "^0.9.12",
    "node-sass": "^4.7.2",
    "opn": "^5.1.0",
